{
  "name": "cf-worker-utils",
  "version": "0.7.0-beta-1",
  "description": "Utilities for writing Cloudflare Workers",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "dev": "npm run --silent build",
    "prepare": "npm run --silent build",
    "prettier": "prettier --write \"./src/**/*.{mjs,js,jsx}\"",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "jest": {
    "setupFiles": [
      "./src/jestSetup.js"
    ]
  },
  "prettier": {
    "useTabs": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/markjaquith/cf-worker-utils.git"
  },
  "keywords": [
    "cloudflare",
    "workers"
  ],
  "sideEffects": false,
  "author": "Mark Jaquith",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/markjaquith/cf-worker-utils/issues"
  },
  "homepage": "https://github.com/markjaquith/cf-worker-utils#readme",
  "dependencies": {
    "cookie": "^0.4.1",
    "lodash": "^4.17.21",
    "path-to-regexp": "^6.2.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.13.5",
    "@cloudflare/workers-types": "^2.1.0",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "husky": "^5.1.1",
    "jest": "^26.6.3",
    "jest-fetch-mock": "^3.0.3",
    "node-fetch": "^2.6.1",
    "pinst": "^2.1.6",
    "prettier": "^2.2.1",
    "service-worker-mock": "^2.0.5",
    "source-map-loader": "^2.0.1",
    "ts-loader": "^8.0.17",
    "typescript": "^4.2.2"
  }
}
